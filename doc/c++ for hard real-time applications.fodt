   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T24">uintptr_t</text:span><text:span text:style-name="T12"> block;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/>block = </text:span><text:span text:style-name="T22">alignAddress</text:span><text:span text:style-name="T12">(</text:span><text:span text:style-name="T39">firstNotAllocatedAddress</text:span><text:span text:style-name="T12">, </text:span><text:span text:style-name="T39">alignment</text:span><text:span text:style-name="T12">);</text:span></text:p>
