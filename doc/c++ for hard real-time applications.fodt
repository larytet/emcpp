   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T3"><text:s text:c="2"/></text:span><text:span text:style-name="T2">inline</text:span><text:span text:style-name="T3"> </text:span><text:span text:style-name="T2">void</text:span><text:span text:style-name="T3"> add(</text:span><text:span text:style-name="T2">const</text:span><text:span text:style-name="T3"> ObjectType object) {</text:span></text:p>
   <text:p text:style-name="P8">mov %rcx,%rdx</text:p>
