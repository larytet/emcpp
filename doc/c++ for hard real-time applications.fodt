   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T5">bool</text:span><text:span text:style-name="T12"> res = </text:span><text:span text:style-name="T5">true</text:span><text:span text:style-name="T12">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/>res = res &amp;&amp; blockPtr &gt;= </text:span><text:span text:style-name="T39">memoryRegion</text:span><text:span text:style-name="T12">.getAddress();</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T24">size_t</text:span><text:span text:style-name="T12"> maxAddress = </text:span><text:span text:style-name="T39">memoryRegion</text:span><text:span text:style-name="T12">.getAddress()+</text:span><text:span text:style-name="T39">sizeTotalBytes</text:span><text:span text:style-name="T12">;</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/>res = res &amp;&amp; (blockPtr &lt;= maxAddress);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T24">uintptr_t</text:span><text:span text:style-name="T12"> alignedAddress = </text:span><text:span text:style-name="T22">alignAddress</text:span><text:span text:style-name="T12">(blockPtr, </text:span><text:span text:style-name="T39">alignment</text:span><text:span text:style-name="T12">);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/>res = res &amp;&amp; (blockPtr == alignedAddress);</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T5">return</text:span><text:span text:style-name="T12"> res;</text:span></text:p>
