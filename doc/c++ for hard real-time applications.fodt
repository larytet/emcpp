   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="8"/></text:span><text:span text:style-name="T5">return</text:span><text:span text:style-name="T12"> (value + ((</text:span><text:span text:style-name="T24">size_t</text:span><text:span text:style-name="T12">)alignment-1)) &amp; (~((</text:span><text:span text:style-name="T24">size_t</text:span><text:span text:style-name="T12">)alignment-1));</text:span></text:p>
   <text:p text:style-name="Preformatted_20_Text"><text:span text:style-name="T12"><text:s text:c="4"/>}</text:span></text:p>
