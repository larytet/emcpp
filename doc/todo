Find number of areas filled with ones in a mattix. 
Breadth first search 
func visitNeighbour(neighbour,color int) bool {
   if map[neighbour] == 0 {
       return False
   }
   map[neighbour] = color
   return True
}

func visitNeighbours(neighbour, color int) {
   for _, neighbour := getAllNeighbours(neighbour) {
   	belongs := visitNeighbour(neighbour,color)
	if belongs {
		visitNeighbours(neighbour,color)
	}
   }
}

color := 2
for row := 0;row <rows;row ++ {
	for col := 0;col <cols;col ++ {
		neighbour := Neighbour{row, col}
		if map[neighbour] != 0 {
			continue
		}
		visitNeighbours(neighbour, color)
		color++
	}
}
fmt.print("%d", color-2)



