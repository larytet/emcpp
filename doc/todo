def mark2(arr):
    max_diff = arr[1] - arr[0];
    entry_p = exit_p = 0
    i = 0
    j = 0
    for i in range(len(arr)):
        for j in range (i+1, len(arr)):
            if(arr[j] - arr[i] > max_diff):   
                max_diff = arr[j] - arr[i];
                entry_p = i
                exit_p = j
    return (entry_p, exit_p)
 
