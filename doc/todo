def mark(quotes):
    entry_p = exit_p = 0
    for i in range(len(quotes)):
        min = entry_p
        max = exit_p
        quote = quotes[i]
        if quote > quotes[max]:
            max = i
        if (quote < quotes[min]):
            min = i
        
        print "max", max, "min", min, "i", i 
        if max > min:
            entry_p = min
            exit_p = max
    
    return (entry_p, exit_p)

